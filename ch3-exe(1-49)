        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "# ایجاد شکل و محور\n",
        "fig, ax = plt.subplots()\n",
        "\n",
        "# رسم دایره واحد\n",
        "theta = np.linspace(0, 2*np.pi, 1000)\n",
        "x = np.cos(theta)\n",
        "y = np.sin(theta)\n",
        "ax.plot(x, y, 'k')  # دایره واحد\n",
        "\n",
        "# رسم منطقه هاشورخورده (داخل دایره)\n",
        "ax.fill(x, y, color='lightgray', hatch='//', edgecolor='black', alpha=0.3)\n",
        "\n",
        "# رسم محورهای مختصات\n",
        "ax.axhline(0, color='black')\n",
        "ax.axvline(0, color='black')\n",
        "\n",
        "# مشخص‌کردن صفرها (دایره‌ها) و قطب‌ها (ضربدرها)\n",
        "zeros = [1.2, np.exp(1j*np.pi/4), np.exp(-1j*np.pi/4)]  # صفرها\n",
        "poles = [0, -4/3]  # قطب‌ها\n",
        "\n",
        "# رسم صفرها\n",
        "for z in zeros:\n",
        "    ax.plot(np.real(z), np.imag(z), 'o', markersize=10, markeredgewidth=2, markeredgecolor='black', markerfacecolor='none')\n",
        "\n",
        "# رسم قطب‌ها\n",
        "for p in poles:\n",
        "    ax.plot(np.real(p), np.imag(p), 'x', markersize=10, markeredgewidth=2, color='black')\n",
        "\n",
        "# برچسب‌گذاری\n",
        "ax.text(-4/3, 0.05, \"-4/3\", fontsize=12)\n",
        "ax.text(0.1, 1.05, \"Y(z)\", fontsize=12)\n",
        "\n",
        "# تنظیمات نمایشی\n",
        "ax.set_aspect('equal')\n",
        "ax.set_xlim(-2, 2)\n",
        "ax.set_ylim(-2, 2)\n",
        "ax.grid(True)\n",
        "plt.title(\"Pole-Zero Plot\")\n",
        "plt.show()"
