        "a = np.zeros_like(n_a)\n",
        "a[n_a % 4 == 0] = 1  # ضربه‌های تناوبی هر 4 نمونه\n",
        "\n",
        "plt.figure(figsize=(10,4))\n",
        "markerline, stemlines, baseline = plt.stem(n_a, a)\n",
        "plt.setp(stemlines, 'linewidth', 2)  # تنظیم ضخامت خطوط\n",
