      "cell_type": "code",
      "source": [
        "# %% [python]\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "# ایجاد نمودار قطب-صفر Y(z)\n",
        "plt.figure(figsize=(8, 6))\n",
        "\n",
        "# رسم دایره واحد\n",
        "unit_circle = plt.Circle((0, 0), 1, fill=False, color='black', linestyle='dashed', linewidth=0.8)\n",
        "plt.gca().add_patch(unit_circle)\n",
        "\n",
        "# تعیین قطب‌ها و صفرها (مقادیر نمونه - باید با تصویر تطبیق داده شوند)\n",
        "poles = [0.5 + 0.5j, 0.5 - 0.5j]  # مقادیر نمونه\n",
        "zeros = [-0.8]  # مقادیر نمونه\n",
        "\n",
        "# رسم قطب‌ها (× قرمز) و صفرها (○ آبی)\n",
        "plt.scatter(np.real(poles), np.imag(poles), marker='x', s=100, color='red', linewidths=2)\n",
        "plt.scatter(np.real(zeros), np.imag(zeros), marker='o', s=100, color='blue', facecolors='none', linewidths=2)\n",
        "\n",
        "# تنظیمات نمودار\n",
        "plt.axhline(0, color='black', linewidth=0.5, alpha=0.5)\n",
        "plt.axvline(0, color='black', linewidth=0.5, alpha=0.5)\n",
        "plt.grid(True, linestyle='--', alpha=0.5)\n",
        "plt.title('Pole-Zero Plot of Y(z)', fontsize=12)\n",
        "plt.xlabel('Real Part', fontsize=10)\n",
        "plt.ylabel('Imaginary Part', fontsize=10)\n",
        "plt.axis('equal')\n",
        "plt.xlim([-2, 2])\n",
        "plt.ylim([-1.5, 1.5])\n",
        "plt.show()"
      ],
      "metadata": {
