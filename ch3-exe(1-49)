        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from matplotlib.patches import Circle\n",
        "\n",
        "# تعریف قطب‌ها و صفرهای H(z)\n",
        "zeros = [2]  # صفرها\n",
        "poles = [2/3]  # قطب‌ها\n",
        "\n",
        "# رسم نمودار\n",
        "plt.figure(figsize=(6, 6))\n",
        "ax = plt.gca()\n",
        "\n",
        "# رسم دایره واحد\n",
        "unit_circle = Circle((0, 0), 1, fill=False, color='black', linestyle='dashed')\n",
        "ax.add_patch(unit_circle)\n",
        "\n",
        "# رسم صفرها\n",
        "plt.scatter(np.real(zeros), np.imag(zeros), marker='o', color='red', label='صفرها')\n",
        "\n",
        "# رسم قطب‌ها\n",
        "plt.scatter(np.real(poles), np.imag(poles), marker='x', color='blue', label='قطب‌ها')\n",
        "\n",
        "# تنظیمات نمودار\n",
        "plt.axhline(0, color='black', linewidth=1)\n",
        "plt.axvline(0, color='black', linewidth=1)\n",
        "plt.grid(True, which='both', linestyle='--', linewidth=0.5)\n",
        "plt.title('نمودار قطب‌ها و صفرهای \\( H(z) \\)')\n",
        "plt.xlabel('بخش حقیقی')\n",
        "plt.ylabel('بخش موهومی')\n",
        "plt.legend()\n",
        "plt.axis('equal')\n",
        "plt.xlim([-1.5, 2.5])\n",
        "plt.ylim([-1.5, 1.5])\n",
        "\n",
        "# نمایش نمودار\n",
        "plt.show()"
