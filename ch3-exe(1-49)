      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "from matplotlib.patches import FancyArrow, Rectangle, Circle\n",
        "\n",
        "fig, ax = plt.subplots(figsize=(10, 3))\n",
        "\n",
        "# تابعی برای رسم فلش\n",
        "def draw_arrow(x1, y1, x2, y2, text='', text_offset=(0, 0.2)):\n",
        "    ax.annotate(\"\",\n",
        "                xy=(x2, y2), xycoords='data',\n",
        "                xytext=(x1, y1), textcoords='data',\n",
        "                arrowprops=dict(arrowstyle=\"->\", lw=1.5))\n",
        "    if text:\n",
        "        ax.text((x1 + x2)/2 + text_offset[0], (y1 + y2)/2 + text_offset[1], text, fontsize=12)\n",
        "\n",
        "# ورودی x[n]\n",
        "draw_arrow(-1, 0, -0.2, 0, 'x[n]')\n",
        "\n",
        "# جمع‌کننده اول\n",
        "circle1 = Circle((0, 0), 0.15, fill=False)\n",
        "ax.add_patch(circle1)\n",
        "ax.text(-0.05, -0.05, '+', fontsize=12)\n",
        "ax.text(-0.05, -0.15, '-', fontsize=12)\n",
        "\n",
        "# خروجی v[n]\n",
        "draw_arrow(0.15, 0, 1.2, 0, 'v[n]')\n",
        "\n",
        "# بلوک H(z)\n",
        "block = Rectangle((1.2, -0.25), 0.6, 0.5, fill=False)\n",
        "ax.add_patch(block)\n",
        "ax.text(1.35, 0, 'H(z)', fontsize=12)\n",
        "\n",
        "# خروجی بلوک -> جمع‌کننده دوم\n",
        "draw_arrow(1.8, 0, 2.4, 0)\n",
        "\n",
        "# جمع‌کننده دوم\n",
        "circle2 = Circle((2.55, 0), 0.15, fill=False)\n",
        "ax.add_patch(circle2)\n",
        "ax.text(2.5, 0.05, '+', fontsize=12)\n",
        "ax.text(2.5, -0.1, '+', fontsize=12)\n",
        "\n",
        "# ورودی e[n] از بالا\n",
        "draw_arrow(2.55, 1, 2.55, 0.15, 'e[n]', text_offset=(0.2, 0.2))\n",
        "\n",
        "# خروجی w[n]\n",
        "draw_arrow(2.7, 0, 3.4, 0, 'w[n]')\n",
        "\n",
        "# مسیر بازخورد w[n] -> جمع‌کننده اول\n",
        "draw_arrow(3.4, 0, 3.4, -1)\n",
        "draw_arrow(3.4, -1, 0, -1)\n",
        "draw_arrow(0, -1, 0, -0.15)\n",
        "\n",
        "# حذف محور‌ها\n",
        "ax.axis('off')\n",
        "ax.set_xlim(-1.5, 4)\n",
        "ax.set_ylim(-1.5, 1.5)\n",
        "\n",
        "plt.title(\"Feedback System Block Diagram (Figure P3.42)\", fontsize=14)\n",
        "plt.show()"
